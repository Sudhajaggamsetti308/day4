sno=int(input('enter sno'))
sname=input('enter sname:')
group=input('enter group name:')
s1=int(input('enter maths marks:'))
s2=int(input('enter physics marks:'))
s3=int(input('enter chemistry marks:'))
total=s1+s2+s3
avg=total/3
if avg>=90 :
    print('O Grade')
elif avg>=80:
    print('A Grade')
elif avg>=70:
    print('B Grade')
elif avg>=60:
    print('C Grade')
elif avg>=50:
    print('D Grade')
else:
    print('pass')
